/ {
	led {
		compatible = "gpio-leds";

		led: led {
			gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
			label = "led";
		};
	};
};

&wifi {
	status = "okay";
};

&pinctrl {

	spim2_custom: spim2_custom {
		group1 {
			pinmux = <SPIM2_MISO_GPIO14>,
					 <SPIM2_SCLK_GPIO27>,
					 <SPIM2_CSEL_GPIO26>;
		};

		group2 {
			pinmux = <SPIM2_MOSI_GPIO25>;
			output-low;
		};
	};

	i2c0_custom: i2c0_custom {
		group1 {
			pinmux = <I2C0_SDA_GPIO33>,
					 <I2C0_SCL_GPIO32>;
			bias-pull-up;
			drive-open-drain;
			output-high;
		};
	};
};

&spi2 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	pinctrl-0 = <&spim2_custom>;
	pinctrl-names = "default";
};

&i2c0 {
	status = "okay";
	clock-frequency = <I2C_BITRATE_STANDARD>;

	sda-gpios = <&gpio0 33 GPIO_OPEN_DRAIN>;
	scl-gpios = <&gpio0 32 GPIO_OPEN_DRAIN>;

	pinctrl-0 = <&i2c0_custom>;
	pinctrl-names = "default";

	fuel_gauge: fuel_gauge@36 {
		compatible = "maxim,max17048";
		status = "okay";
		reg = <0x36>;
	};
};

&spi3 {
	status = "disabled";
};